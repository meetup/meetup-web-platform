{
  "name": "meetup-web-platform",
  "version": "0.2.0",
  "description": "A collection of Node web application utilities developed for the Meetup web platform",
  "scripts": {
    "test": "jest",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "jest": {
    "automock": false,
    "coverageDirectory": "<rootDir>/coverage",
    "setupFiles": [
      "<rootDir>/util/globals.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testRegex": ".*\\.test\\.jsx?$"
  },
  "dependencies": {
    "accepts": "^1.3.3",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "boom": "^3.2.2",
    "chalk": "^1.1.3",
    "eslint": "^3.3.1",
    "eslint-plugin-react": "^6.1.2",
    "good": "^7.0.1",
    "good-console": "^6.1.2",
    "good-squeeze": "^4.0.0",
    "hapi": "^14.2.0",
    "idb": "^1.1.4",
    "inert": "^4.0.2",
    "intl": "^1.2.4",
    "jest-cli": "^14.1.0",
    "js-cookie": "^2.1.2",
    "node-fetch": "^1.6.0",
    "node-uuid": "^1.4.7",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-helmet": "^3.1.0",
    "react-intl": "^2.1.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-observable": "^0.9.1",
    "request": "^2.74.0",
    "rxjs": "^5.0.0-beta.11",
    "source-map-support": "^0.4.2",
    "url-search-params": "^0.5.0"
  }
}
