{
  "name": "mwp-core",
  "description": "A collection of Node web application utilities developed for the Meetup web platform",
  "engineStrict": true,
  "engines": {
    "node": ">=7.0.0"
  },
  "main": "./lib/index.js",
  "config": {
    "description": "This config is used to mock a consumer app config",
    "subdomain": "mwp-foo",
    "agent": "MUP_WEB"
  },
  "scripts": {
    "build": "babel src --out-dir lib --ignore test.js,test.jsx --source-maps",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "flow": "flow",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test": "yarn run flow && yarn run test:unit -- --silent --coverage && yarn run test:integration -- --silent",
    "test:integration": "NODE_ENV=test jest --config jest.int.config.json",
    "test:unit": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "yarn test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/meetup-web-platform.git"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/meetup-web-platform/issues"
  },
  "homepage": "https://github.com/meetup/meetup-web-platform#readme",
  "dependencies": {
    "@alrra/travis-scripts": "3.0.1",
    "@google-cloud/pubsub": "0.13.1",
    "accepts": "1.3.3",
    "avsc": "5.0.4",
    "boom": "5.1.0",
    "chalk": "2.0.1",
    "convict": "4.0.0",
    "electrode-csrf-jwt": "1.2.1",
    "escape-html": "1.0.3",
    "hapi": "16.4.3",
    "hapi-pino": "1.7.0",
    "idb-keyval": "2.3.0",
    "inert": "4.2.0",
    "intl": "1.2.5",
    "joi": "10.6.0",
    "js-cookie": "2.1.4",
    "mwp-router": "6.0.1604-beta",
    "node-fetch": "1.7.1",
    "pino": "4.6.0",
    "prop-types": "15.5.10",
    "qs": "6.5.0",
    "react-dom": "15.6.1",
    "react-side-effect": "1.1.3",
    "redbox-react": "1.4.3",
    "redux-observable": "0.14.1",
    "request": "2.81.0",
    "rison": "0.1.1",
    "source-map-support": "0.4.15",
    "spdy": "3.4.7",
    "url-regex": "3.2.0",
    "uuid": "3.1.0"
  },
  "peerDependencies": {
    "mwp-cli": "^1.0.101"
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "enzyme": "2.9.1",
    "intl-messageformat": "2.0.0",
    "intl-relativeformat": "2.0.0",
    "iron": "4.0.5",
    "jest": "20.0.4",
    "meetup-web-mocks": "1.0.204",
    "mwp-cli": "1.0.101",
    "prettier": "1.5.2",
    "react": "15.6.1",
    "react-helmet": "5.1.3",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-test-renderer": "15.6.1",
    "redux": "3.7.1",
    "rxjs": "5.4.2",
    "tough-cookie": "2.3.2"
  }
}
